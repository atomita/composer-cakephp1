{
	"name": "atomita/composer-cakephp1",
	"description": "",
	"keywords": ["cakephp"],
	"license": "MIT",
	"authors": [
		{
			"name": "atomita"
		}
	],
	"version": "0.0.1",
	"require": {
		"cakephp/cakephp": ">=1.3.17, <2",
		"php": ">=5",
		"cakephp/debug_kit": "1.3.*",
		"atomita/loader-of-less-than-php5.3-plugin": "dev-master",
		"atomita/cakephp1-setup-plugin": "dev-master"
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "cakephp/cakephp",
				"version": "1.3.17",
				"dist": {
					"url": "https://github.com/cakephp/cakephp/archive/1.3.17.zip",
					"type": "zip"
				},
				"source": {
					"url": "https://github.com/cakephp/cakephp.git",
					"type": "git",
					"reference": "tags/1.3.17"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "cakephp/debug_kit",
				"version": "1.3.0",
				"dist": {
					"url": "https://github.com/cakephp/debug_kit/archive/1.3.0.zip",
					"type": "zip"
				},
				"source": {
					"url": "https://github.com/cakephp/debug_kit.git",
					"type": "git",
					"reference": "tags/1.3.0"
				}
			}
		}
	],
	"extra": {
		"cakephp1plugins": {
			"app": ["cakephp/debug_kit"]
		}
	}
}
